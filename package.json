{
	"name": "@fe-hl/monitor-sdk",
	"version": "1.2.3",
	"private": false,
	"description": "前端监控SDK",
	"main": "lib/index.common.js",
	"module": "lib/index.esm.js",
	"browser": "lib/index.browser.js",
	"typings": "lib/types/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"dev": "cross-env NODE_ENV=development rollup -w -c rollup.config.js",
		"build": "rimraf lib && cross-env NODE_ENV=production rollup -c rollup.config.js",
		"server": "cross-env NODE_ENV=development rollup -w -c rollup.config.dev.js",
		"prepare": "husky install", 
		"mock": "nodemon examples/app.js"
	},
	"repository": {
		"type": "git",
		"url": "https://gitee.com/fe-hl/fe-monitor-sdk.git"
	},
	"keywords": [
		"TypeScript",
		"rollup"
	],
	"author": "hulei",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.18.13",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-buble": "^0.21.3",
		"@rollup/plugin-commonjs": "^22.0.2",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@types/big.js": "^6.1.5",
		"@types/ua-parser-js": "^0.7.36",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.4.0",
		"@typescript-eslint/parser": "^5.4.0",
		"@vue/cli-plugin-eslint": "~5.0.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cz-customizable": "^6.9.2",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-html": "^7.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"fs-extra": "^11.1.1",
		"husky": "^8.0.1",
		"koa": "^2.13.4",
		"koa-body": "^6.0.1",
		"koa-router": "^12.0.0",
		"koa-static": "^5.0.0",
		"koa2-cors": "^2.0.6",
		"nodemon": "^2.0.20",
		"prettier": "^2.4.1",
		"rollup": "^2.78.1",
		"rollup-plugin-alias": "^2.2.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.33.0",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"lint-staged": {
		"src/**/*.{js,ts,vue}": [
			"eslint --fix",
			"git add ."
		]
	},
	"dependencies": {
		"bowser": "^2.11.0",
		"core-js": "^3.25.0",
		"ua-parser-js": "^1.0.32",
		"uuid": "^9.0.0",
		"web-vitals": "^3.1.0"
	}
}
